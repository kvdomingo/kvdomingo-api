{% extends 'web/base.html.j2' %}

{% block body %}

<link rel="stylesheet" href="{{ static('web/css/cv.css') }}" />

<div class="container px-md-5 pb-4">

    <h1 class="h2-responsive py-4"><i class="fas fa-graduation-cap mr-3"></i>Education</h1>
    <div class="container px-md-5">
        <ul class="timeline" id="education"></ul>
    </div>

    <h1 class="h2-responsive py-4"><i class="fas fa-briefcase mr-3"></i>Work Experience</h1>
    <div class="container px-md-5">
        <ul class="timeline" id="work"></ul>
    </div>

    <h1 class="h2-responsive py-4"><i class="fas fa-project-diagram mr-3"></i>Projects</h1>
    <div class="container px-md-5">
        <ul class="timeline" id="projects"></ul>
    </div>

</div>

<script src="{{ static('web/js/EducationTimeline.js') }}"></script>
<script src="{{ static('web/js/WorkTimeline.js') }}"></script>
<script src="{{ static('web/js/ProjectTimeline.js') }}"></script>
<script src="{{ static('web/js/cv.js') }}"></script>

{% endblock %}
